    <dc-schema>
        <name>cris</name>
        <namespace>https://www.4science.it/dspace-cris/</namespace>
    </dc-schema>

    <dc-type>
        <schema>cris</schema>
        <element>customurl</element>
        <qualifier></qualifier>
