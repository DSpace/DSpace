        <namespace>http://purl.org/dspace/models/journal</namespace>
    </dc-schema>


    <dc-type>
        <schema>journal</schema>
        <element>journalID</element>
    </dc-type>

    <dc-type>
        <schema>journal</schema>
        <element>metadataDir</element>
    </dc-type>

    <dc-type>
        <schema>journal</schema>
        <element>parsingScheme</element>
    </dc-type>

    <dc-type>
        <schema>journal</schema>
        <element>integrated</element>
    </dc-type>

    <dc-type>
        <schema>journal</schema>
        <element>allowReviewWorkflow</element>
    </dc-type>

    <dc-type>
