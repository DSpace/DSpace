        <element>issn</element>
    </dc-type>

    <dc-type>
        <schema>journal</schema>
        <element>hasJournalPage</element>
    </dc-type>

    <dc-type>
        <schema>journal</schema>
        <element>coverImage</element>
    </dc-type>
</dspace-dc-types>
