    dm:object dm:DSpaceValue ;
  ] ;
  .

:available
  dm:metadataName "dc.date.available" ;
  dm:creates [
    dm:subject dm:DSpaceObjectIRI ;
    dm:predicate dcterms:available ;
    dm:object [
      a dm:LiteralGenerator ;
      dm:pattern "$DSpaceValue" ;
      dm:literalType xsd:dateTime ;
    ] ;
  ] ;
  .

:dateCopyrighted
