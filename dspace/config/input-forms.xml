                </field>

                <!-- is published -->
                <field>
                    <dc-schema>cristin</dc-schema>
                    <dc-element>ispublished</dc-element>
                    <dc-qualifier></dc-qualifier>
                    <repeatable>false</repeatable>
                    <label>Is Published?</label>
